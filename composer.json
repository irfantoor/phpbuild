{
    "name": "irfantoor/phpbuild",
    "description": "PHP Build your composer projects from a json file",
    "keywords": ["irfan", "php", "build", "json"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfan TOOR",
            "email": "email@irfantoor.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "irfantoor/engine": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "bin": [
        "phpbuild"
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "IrfanTOOR\\Engine\\": "src"
        }
    }
}
